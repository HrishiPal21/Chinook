									"referenceName": "SnowflakeChinookDB",
									"type": "DatasetReference",
									"parameters": {
										"schema_name": "STAGE",
										"table_name": {
											"value": "@toUpper(split(item(), '.')[1])",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"table_list": {
				"type": "array",
				"defaultValue": [
					"chinook.Invoice",
					"chinook.InvoiceLine"
				]
			}
		},
